{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/(.*\\.(css|js|png|jpg|jpeg|svg|mp4|gif|ico))",
            "dest": "/public/$1"
        },
        {
            "src": "/",
            "dest": "/api/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/$1"
        }
    ]
}